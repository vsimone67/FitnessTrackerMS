<?xml version="1.0" encoding="utf-8" ?>
<d:BaseView xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="FitnessTracker.Mobile.Views.AddBodyInfo"
            xmlns:d="clr-namespace:FitnessTracker.Mobile.Views;assembly=FitnessTracker.Mobile.Views"
            xmlns:viewModelBase="clr-namespace:FitnessTracker.Mobile.ViewModels;assembly=FitnessTracker.Presentation.Mobile"
            viewModelBase:ViewModelLocator.AutoWireViewModel="true"
            Title="{Binding Title}">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Xamarin.Forms!"
                VerticalOptions="CenterAndExpand"
                HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</d:BaseView>